<mvvm:MvvmApplication x:TypeArguments="viewModels:AppBootstrapperViewModel" 
                      x:Class="FastReport.Designer.App"
                      xmlns="https://github.com/avaloniaui"
                      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                      xmlns:actipro="http://schemas.actiprosoftware.com/avaloniaui"
                      xmlns:viewModels="clr-namespace:FastReport.Designer.ViewModels"
                      xmlns:avalonia="clr-namespace:Material.Icons.Avalonia;assembly=Material.Icons.Avalonia"
                      xmlns:mvvm="https://github.com/rftd/Caramelo.MvvmApp"
                      RequestedThemeVariant="Light">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceInclude Source="avares://LoadingIndicators.Avalonia/LoadingIndicators.axaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
    <Application.Styles>
        <actipro:ModernTheme />
        <avalonia:MaterialIconStyles />
    </Application.Styles>
</mvvm:MvvmApplication>